android:layout_width="match_parent"
android:layout_height="wrap_content"

tools:context=".video_sign_in">

    <VideoView
        android:id="@+id/videoView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="250dp"
